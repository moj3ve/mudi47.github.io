<package>
	<id>com.mudi.ytplus</id>
	<name>YT+</name>
	<version>1.0</version>
	<compatibility>
		<firmware>
			<miniOS>11.0</miniOS>
			<maxiOS>13.3</maxiOS>
			<otherVersions>unsupported</otherVersions>
		</firmware>
	</compatibility>
	<dependencies>
		<package>mobilesubstrate (>= 0.9.5000)</package>
		<package>preferenceloader</package>
